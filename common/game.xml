    </background>

    <effects>
        <coin_arrived filename="common/particles/coins_holder_particle.psi">
            <sprite sprite_id="particle:game" />
        </coin_arrived>
    </effects>

    <map_stack_items>
        <stack id="currencyStack" layout="horizontal">
            <nine_patch sprite_id="ui_ninepatch_currency_bar:game" patch="6 6 8 8" size="150 40">
