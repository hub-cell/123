    <gui>
        <alignment id="hud" v_align="0" h_align="0">
            <alignment h_align="right">
                <stack id="rightStack" alignment="right" layout="vertical" padding="10" anchor="1 0" position="-10 10" />
            </alignment>
            <alignment h_align="left">
                <stack id="leftStack" alignment="left" layout="vertical" padding="10" anchor="0 0" position="10 10" />
            </alignment>
            <alignment h_align="1">
                <stack id="stack" layout="vertical" position="0 5" padding="5">
                    <nine_patch id="task" sprite_id="ui_ninepatch_frame:game" patch="6 6 8 8" anchor="0.5" autosize="true" padding="20 10">
                        <stack id="grpTask" visible="0" layout="horizontal" padding="5">
                            <image id="icon_chest" sprite_id="ui_tasks_icon_chest:game" />

                            <stack layout="vertical" padding="5">
                                <text_multiline id="txtMessage" size="200 40" font="common/fonts/NotoSans-CondensedMedium.ttf" font_size="18" stretchmode="2" />
                                <progress id="progress" position="100 50" sprite_id="ui_tasks_progress_back:game" sprite_id_filler="ui_tasks_progress:game" offset="1 1" />
                            </stack>

                            <image id="timer" sprite_id="ui_tasks_timer_back:game">
                                <text id="txtTimer" position="8 14" size="34 16" font="common/fonts/NotoSans-CondensedMedium.ttf" font_size="50" stretchmode="2" />
                                <clock id="clockTimer" sprite_id="ui_powerups_progress:game" position="24 24" />
                            </image>
                        </stack>

                        <stack id="grpResult" visible="0" layout="horizontal">
                            <text id="task_no_room" size="290 25" font="common/fonts/NotoSans-CondensedMedium.ttf" font_size="50" stretchmode="2" trim_container="true">
                                <image rect="0 0 1 50" hotspot="0 25" position="0 17" color="0" />
                            </text>

                            <image id="separator" rect="0 0 60 60" color="0" />

                            <text id="task_done" size="235 25" font="common/fonts/NotoSans-CondensedMedium.ttf" font_size="50" stretchmode="2" color="ff10ff10" trim_container="true">
                                <image sprite_id="ui_tasks_icon_done:game" position="-30 14" />
                            </text>

                            <text id="task_failed" size="235 25" font="common/fonts/NotoSans-CondensedMedium.ttf" font_size="50" stretchmode="2" color="ffff1010" trim_container="true">
                                <image sprite_id="ui_tasks_icon_failed:game" position="-30 14" />
                            </text>
                        </stack>
                    </nine_patch>

                    <stack_item size="0 55">
                        <powerups id="powerups" position="0 25" />
                    </stack_item>
                </stack>
            </alignment>

            <alignment v_align="2" h_align="1">
                <game_logs id="game_logs" visible="0" position="0 -140">
                    <nine_patch sprite_id="ui_ninepatch_frame:game" patch="6 6 8 8" anchor="0.5" autosize="true" padding="20 10">
                        <bounds>
                            <image rect="0 0 40 50" color="0">
                                <image id="iconCoin" sprite_id="icon_coin:common" position="-36 27" scale="0.62" />
                                <image id="iconFace" sprite_id="ui_icon_ghost:game" position="-36 25" scale="0.78" />
                                <image id="iconExcl" sprite_id="ui_icon_excl:game" position="-36 25" />
                                <image id="iconPig" sprite_id="piggybank_pig:menu" position="-36 25" scale="0.7" />
                            </image>
                            <text id="txtMessage" position="0 3" size="400 32" font="common/fonts/NotoSans-CondensedMedium.ttf" font_size="50" stretchmode="2" trim_container="true" />
                        </bounds>
                    </nine_patch>
                </game_logs>

                <layout id="bonuses" layout="horizontal" size="300 100" item_size="90 90" position="-150 -100">
                    <button id="magnet" rect="0 0 88 88" color="0" pressed_scale="0.97" hotspot="44 44" visible="0">
                        <colorcorrection id="color">
                            <image sprite_id="ui_boosters_place:game" position="44 44" />
                            <image sprite_id="ui_boosters_magnet:game" position="44 44" />
                            <text id="txtCount" size="12 25" position="67 56" font_size="24" font="common/fonts/NotoSans-CondensedBlack.ttf" color="ff001d67" />
                        </colorcorrection>
                    </button>
                    <button id="speedup" rect="0 0 88 88" color="0" pressed_scale="0.97" hotspot="44 44" visible="0">
                        <colorcorrection id="color">
                            <image sprite_id="ui_boosters_place:game" position="44 44" />
                            <image sprite_id="ui_boosters_speedup:game" position="44 44" />
                            <text id="txtCount" size="12 25" position="67 56" font_size="24" font="common/fonts/NotoSans-CondensedBlack.ttf" color="ff001d67" />
