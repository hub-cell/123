            </nine_patch>
        </stack>
        <stack id="mapStack" layout="vertical">
            <nine_patch id="mapPatch" sprite_id="ui_ninepatch_map:game" patch="3 3 0 0">
                <map id="map" position="4 4" />
            </nine_patch>
            <nine_patch id="nodeTimer" visible="0" position="-5 5" sprite_id="ui_x2_bg:game" hotspot="119 0" patch="6 6 4 4" size="119 40">
                <image id="imgX2" sprite_id="ui_x2:game" position="10 22" hotspot="26 20"/>
