    <lastchance_popup>
        <autoload size="522 550" shade="1" shade_color="ca000000" overscale="1.1" speed="10" direction="0 0">
            <nine_patch sprite_id="nine_patch_result_back:common" hotspot="0" patch="28 28 28 28" size="580 470" scale="0.9">
                <nine_patch sprite_id="nine_patch_back_top:common" patch="20 0 20 0" size="570 67" position="4 6" hotspot="0 0">
                    <text id="lastchance_popup_title" position="65 2" size="450 46" font="common/fonts/NotoSans-CondensedMedium.ttf" font_size="100" color="ffffffff" stretchmode="2" />
                </nine_patch>

                <image sprite_id="shine:common" position="290 200">
                    <image sprite_id="icon_worm_with_stars:common" position="118 118" />
                </image>

                <image sprite_id="clock_progress:common" position="290 395" color="ff102b5c">
                    <text id="txtTimeout" size="70 50" position="15 18" font="common/fonts/NotoSans-CondensedMedium.ttf" font_size="100" color="ffffffff" stretchmode="2" />
                    <clock id="clockTimer" sprite_id="clock_progress:common" position="50 50" color="ffffde00" />
                </image>

                <text id="sessiontimeout_popup_tip" position="140 303" size="300 25" font="common/fonts/NotoSans-CondensedMedium.ttf" font_size="50" color="ff15aadc" stretchmode="2" />

                <alignment v_align="2" h_align="1">
                    <button id="btnClose" visible="0" rect="0 0 300 50" hotspot="150 0" position="0 -50" pressed_scale="0.97" color="0">
