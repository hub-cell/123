                        <text id="txtPrice" position="100 15" size="130 50" font="common/fonts/NotoSans-CondensedMedium.ttf" font_size="100" color="ffffffff" h_align="1" v_align="1" stretchmode="2" shadow_offset="1 1" shadow_color="90000000" />
                    </button>

